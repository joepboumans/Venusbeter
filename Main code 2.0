#include <Servo.h>
//Constants
 const float speed = 0.23;
 const float rotatespeed= 0.50 ;
 int RS=A0;
 int LS=A1;
 int RIGHTSERVO=13;
 int LEFTSERVO=12;
 float forwardtime=;
 
//Functions
void Forward(float distanceF, float forwardspeedF) {
  float result;
  result = (distanceF / forwardspeedF) * 1000;
  servoLeft.writeMicroseconds(1900);         // Left wheel counterclockwise
  servoRight.writeMicroseconds(1000);
  delay(result);
}
void TurnLeft(int degrees) {
    float result;
    result = (degrees / (rotatespeed * 360)) * 1000;
    Serial.println("Result computed: ");
    Serial.print(result);
    Serial.println();
    delay(100);
    servoLeft.write(0);
    servoRight.write(0);
    delay(result);
}

void TurnRight(int degrees){
float result;
    result = (degrees / (rotatespeed * 360)) * 1000;
    Serial.println("Result computed: ");
    Serial.print(result);
    Serial.println();
    delay(100);
    servoLeft.write(180);
    servoRight.write(180);
    delay(result);
}

void Stop() {
    servoLeft.write(90);
    servoRight.write(90);
}


int CheckRock(int PinLoc){
  if(digitalRead(PinLoc) ==0){
    return 0;
  }else{ return 1;
 }
}

void check(){
if (CheckRock(US)<=5){
Stop();
pickup(forwardtime);
ReturnLab();
return;
}

\\Setup

void setup(){
 servoEyes.attach(11);
  servoEyes.write(90);
  servoRight.attach(13);
  servoLeft.attach(12);
  servoGripper.attach(10);// attaches the servo on pin 9 to the servo object
  servoGripper.write(90);
  pinMode(encoderLeft, INPUT);
  pinMode(encoderRight,INPUT);
  pinMode(LS, INPUT);
  pinMode(RS, INPUT);
  pinMode(LEFTSERVO, OUTPUT);
  pinMode(RIGHTSERVO, OUTPUT)
  Serial.begin(9600);
  
}

\\loop
void loop(){
if(CheckGround(LS)==0 && CheckGround(RS)==0){
    Forward(4,speed);
  }
  else if(CheckGround(LS) && !CheckGround(RS)){// Turn 
    TurnRight(130);   
  }
  
}
